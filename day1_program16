Wind chill factor:
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a,b;
    double c;
   scanf("%d %d",&a,&b);
    c=35.74+0.6215*a+(0.4275*a-35.75)*pow(b,0.16);
    printf("%.2f\n",c);
    return 0;
}
