Gardening;
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
int main() {
int rows, cols, tree_num;
scanf("%d %d %d", &rows, &cols, &tree_num);
int col = (tree_num - 1) / rows + 1; 
if (col == 2 || col == cols - 1) {
printf("It is a mango tree\n");
}
else
{
printf("It is not a mango tree\n");
}
return 0;
}
